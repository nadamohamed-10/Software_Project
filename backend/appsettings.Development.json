{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=ClinicSystemDb_Dev;Trusted_Connection=True;Encrypt=false;"
  },
  "JwtSettings": {
    "SecretKey": "DevelopmentSecretKeyThatIsAtLeast32CharactersLong!!",
    "ExpirationMinutes": 1440,
    "Issuer": "ClinicAPI",
    "Audience": "ClinicApp"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}
